<struts>


	<package name="shopLabel" namespace="/shopLabel" extends="pinju-auth">
		<!-- 点击更改域名按钮操作 -->
		<action name="toShopLabel"
			class="com.yuwang.pinju.web.module.shop.action.ShopLabelAction"
			method="toShopLabel">
			<result name="success" type="freemarker">/default/templates/screen/shop/shopLabel.ftl</result>
		</action>
		<action name="updateShopLabel"
			class="com.yuwang.pinju.web.module.shop.action.ShopLabelAction"
			method="updateShopLabel">
			<result name="success" type="freemarker">/default/templates/screen/shop/shopLabel.ftl</result>
		</action>
	</package>

</struts>