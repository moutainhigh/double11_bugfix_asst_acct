<?xml version="1.0" encoding="UTF-8" ?>
  <!DOCTYPE struts PUBLIC
      "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
      "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="cate" namespace="/cate" extends="pinju-default">
		<action name="resetCate"
			class="com.yuwang.pinju.web.module.item.screen.ResetCate" method="execute">
			<result name="success" type="freemarker">/default/templates/screen/item/categoryReset.ftl</result>
			<result name="error" type="freemarker">/default/templates/screen/error/error.ftl</result>
		</action>
		
		<action name="cate_level_list"
			class="com.yuwang.pinju.web.module.item.screen.GetCategory" method="getCategoryLevel">
			<result name="success" type="freemarker">/default/templates/screen/item/categoryReset.ftl</result>
			<result name="error" type="freemarker">/default/templates/screen/error/error.ftl</result>
		</action>
	</package>
	
</struts>